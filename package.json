{
    "name": "consento-theme",
    "description": "This is Consento, an adaptation of the Editorial theme for ghost",
    "demo": "https://consento.org",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=2.0.0",
        "ghost-api": "v2"
    },
    "private": true,
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg"
    },
    "scripts": {
        "dev": "gulp",
        "zip": "gulp zip",
        "pretest": "gulp build",
        "test": "gscan .",
        "preship": "yarn test",
        "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then yarn version && git push --follow-tags; fi"
    },
    "author": {
        "name": "Martin Heidegger",
        "email": "martin.heidegger@gmail.com",
        "url": "https://consento.org"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/consento-org/ghost-theme.git"
    },
    "bugs": "https://github.com/consento-org/ghost-theme/issues",
    "contributors": [
        {
            "name": "ajlkn",
            "email": "aj@lkn.io",
            "url": "https://twitter.com/ajlkn"
        },
        {
            "name": "Ghost Foundation",
            "email": "hello@ghost.org",
            "url": "https://ghost.org"
        }
    ],
    "devDependencies": {
        "beeper": "^1.1.1",
        "gscan": "^2.0.0",
        "gulp": "4.0.0",
        "gulp-livereload": "4.0.1",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-watch": "5.0.1",
        "gulp-zip": "4.2.0",
        "node-sass": "^4.10.0",
        "jquery": "^3.4.1",
        "popper.js":"^1.16.0",
        "bootstrap": "^4.4.1"
    },
    "config": {
        "posts_per_page": 6
    }
}
